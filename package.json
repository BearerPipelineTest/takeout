{
  "name": "@tighten/takeout",
  "description": "Docker-based development-only dependency manager.",
  "version": "2.0.0-alpha.1",
  "authors": [
    {
      "name": "Matt Stauffer",
      "email": "matt@tighten.co"
    },
    {
      "name": "Jose Soto",
      "email": "jose@tighten.co"
    }
  ],
  "bin": {
    "takeout": "./bin/run"
  },
  "bugs": "https://github.com/tighten/takeout/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "axios": "^0.21.0",
    "chalk": "^4.1.0",
    "cross-spawn": "^7.0.3",
    "dockerode": "^3.2.1",
    "inquirer": "^7.3.3",
    "sinon": "^9.2.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.49",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nock": "^13.0.5",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/tighten/takeout",
  "keywords": [
    "docker",
    "dependencies",
    "dependency management",
    "database",
    "development"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "takeout",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tighten/takeout",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
